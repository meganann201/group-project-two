{{#each category.recipes}}
<div class="card" style="width: 18rem;">
  <img class="img-thumbnail card-img-top" src="{{this.image}}" alt="Card image cap">
  <div class="card-body">
    <h5 class="card-title">{{this.recipe_name}}</h5>
    <p class="card-text"> {{this.description}} </p>
    <a href="/recipe/{{this.id}}" class="btn btn-primary">View Recipe</a>
  </div>
</div>
{{/each}}